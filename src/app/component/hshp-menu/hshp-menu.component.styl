@import "~src/stylus/core/variables.styl"

.contenedor
  background-color: transparent
  height: 100vh
  width: 100%

  .contenedor-logo
    background-color: transparent
    height: 68px
    width: 100%

    img
      height: auto
      padding: 5px 0px
      width: 100%

  .contenedor-modulo
    background-color: transparent
    height: calc(100vh - 68px * 2 - 4px * 2)
    padding: 5px 0px
    text-align: center
    width: 100%

    button.btn-cube
      height: 50px
      margin: 7px 2px
      min-width: auto
      padding: 0px
      width: 50px

      .mat-icon
        font-size: 30px
        height: 30px
        width: 30px

      span
        font-size: 16px
        font-weight: 600

  .contenedor-power-off
    background-color: transparent
    padding-top: 9px
    text-align: center
